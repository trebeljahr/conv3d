  "main": "dist/3d-model-converter.js",
  "bin": {
    "conv3d": "./dist/3d-model-converter.js"
  },
  "module": "dist/3d-model-converter.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trebeljahr/conv3d.git"
  },
  "bugs": {
    "url": "https://github.com/trebeljahr/conv3d/issues"
  },
  "homepage": "https://github.com/trebeljahr/conv3d#readme",
  "scripts": {
    "build": "tsc"
  },
  "keywords": [
    "3d",
    "gltf",
    "fbx",
    "obj",
    "three.js",
    "react-three-fiber",
    "cli"
  ],
  "author": "Rico Trebeljahr",
  "license": "MIT",
