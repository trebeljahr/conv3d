    "gltf-pipeline": "^4.1.0",
    "gltfjsx": "^6.5.3",
    "inquirer": "^8.2.4",
    "lolcatjs": "^2.4.3",
    "obj2gltf": "^3.1.6",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.5",
    "@types/inquirer": "^8.2.5",
    "@types/lolcatjs": "^2.4.1",
    "@types/node": "^18.11.9",
    "typescript": "^4.8.4"
  }
}
